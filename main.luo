local function startKeyVerification()
    local saved=g()
    if saved then
        n("Verifying","Checking saved key...")
        local st=api.check_key(saved)
        if st.code=="KEY_VALID"then
            if st.data.note==""then
                n("Success","Saved key valid! Time left: "..string.format("%dm %dd %dh %dm %ds",f(st.data.auth_expire)))
                accessTimeRemaining = st.data.auth_expire
                keyVerified = true
                dtc.pushautoexec()
                initializeFrostWare()
            else
                n("U Need Reset HWID",st.data.note)
                u()
            end
        else
            n("Error","Saved key invalid!")
            u()
        end
    else
        u()
    end
end

