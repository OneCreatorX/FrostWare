function initializeFrostWare()
    if not keyVerified then
        return
    end
    
    local ifw=function()
        local ui,oc=w.cbu()
        spawn(function()
            while wait(1)do 
                w.ut()
            end 
        end)
        w.sh()
        w.iwa()
        w.startAntiAfk()
        w.startAutoRejoin()
        if abn then abn.BackgroundColor3=are and Color3.fromRGB(93,117,160)or Color3.fromRGB(166,190,255)end 
        if bbn then bbn.BackgroundColor3=ben and Color3.fromRGB(93,117,160)or Color3.fromRGB(166,190,255)end 
        if ncn then ncn.BackgroundColor3=nce and Color3.fromRGB(93,117,160)or Color3.fromRGB(166,190,255)end 
        oc.MouseButton1Click:Connect(function()w.sh()end)
        spawn(function()
            repeat wait(0.1)until game:IsLoaded()and game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            w.autoExec()
        end)
    end
    ifw()
end

