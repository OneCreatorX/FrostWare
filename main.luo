local gvt=function()
    if keyVerified and accessTimeRemaining > 0 then
        local currentTime = os.time()
        local remaining = accessTimeRemaining - currentTime
        if remaining <= 0 then
            return 0, 0, 0
        end
        local h = math.floor(remaining / 3600)
        local m = math.floor((remaining % 3600) / 60)
        local s = remaining % 60
        return h, m, s
    else
        return 0, 0, 0
    end
end

